<Window x:Class="ExcelTools.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Excel Tools" Height="450" Width="800">
    <Grid>
        <GroupBox Header="File" Width="200" HorizontalAlignment="Right" Margin="5">
            <StackPanel>
                <TextBox IsReadOnly="True" Margin="0,5,0,0"></TextBox>
                <Button Margin="0,5,0,0" Content="Load"/>
            </StackPanel>
        </GroupBox>
    </Grid>
</Window>
